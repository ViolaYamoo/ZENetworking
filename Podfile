# Uncomment this line to define a global platform for your project
platform :ios, ‘8.0’
use_frameworks!

target 'ZENetworking' do
  # Comment this line if you're not using Swift and don't want to use dynamic
  
    pod 'FMDB'
    pod 'MJRefresh'                 #上拉加载下拉刷新
    pod 'MBProgressHUD'             #等待动画指示
    pod 'YYCache'
    
    pod 'Kingfisher'                #图片下载、缓存的框架
    pod 'Alamofire'                 #网络请求类
    pod 'SwiftyJSON'

  # Pods for ZENetworking

  target 'ZENetworkingTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'ZENetworkingUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
